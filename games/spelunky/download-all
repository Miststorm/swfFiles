# Download index.html
rm index.html
curl -O http://tinysubversions.com/game/spelunky/index.html

rm -r -f html5game
mkdir html5game

# Download Root Directory level files
cd html5game
curl -O http://tinysubversions.com/game/spelunky/html5game/spelunky.js
curl -O http://tinysubversions.com/game/spelunky/html5game/loadingscreen.png
curl -O http://tinysubversions.com/game/spelunky/html5game/spelunky_texture_0.png
curl -O http://tinysubversions.com/game/spelunky/html5game/stats.txt
curl -O http://tinysubversions.com/game/spelunky/html5game/settings.cfg
curl -O http://tinysubversions.com/game/spelunky/html5game/keys.cfg
curl -O http://tinysubversions.com/game/spelunky/html5game/gamepad.cfg
cd ../

